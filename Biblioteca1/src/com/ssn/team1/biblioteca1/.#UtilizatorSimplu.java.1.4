public abstract class UtilizatorSimplu extends Utilizator {
  protected String nume;
  protected String numarTelefon;
  protected ArrayList<Imprumut> istoricImprumuturi;
  protected ArrayList<Exemplar> cartiImprumutate;
  protected int numarDeImprumuturi;

  public ArrayList<Exemplar> getCartiImprumutate() {

    return cartiImprumutate;
  }

  public boolean areCartiNereturnate() {

    for (Exemplar e : cartiImprumutate) {
      if (e.isTermenDepasit()) {
        return true;
      }
    }

    return false;
  }

}
